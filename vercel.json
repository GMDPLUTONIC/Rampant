{
    "functions": {
      "api/**/*.php": {
        "runtime": "vercel-php@0.5.2"
      }
    },
    "routes": [
      { "src": "/(.*)",  "dest": "/api/index.php" },
      { "src": "/demon",  "dest": "/api/demon/index.php"},
      { "src": "/impossible", "dest": "/api/impossible/index.php"},
      { "src": "/possible", "dest": "/api/possible/index.php"},
      { "src": "/assets/css/main.css", "dest": "/api/assets/css/main.css" }]
  }